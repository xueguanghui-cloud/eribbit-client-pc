{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "include": ["vite.config.*", "vitest.config.*", "cypress.config.*"],
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist/",
    "declaration": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES6",
    "allowJs": false,
    "types": [
      "node",
      "webpack-env"
    ]
    
  }
}
